<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
		<plugin>

            <!-- the plugin name [A-Za-z_] -->
            <pluginName>Filestack</pluginName>

            <!-- a short description -->
			<pluginDescription><![CDATA[Allow uploading assets via the Filestack API service]]></pluginDescription>

            <!-- meta data -->
            <pluginVersion>1.0</pluginVersion>
            <pluginRevision>1</pluginRevision>
            <pluginBuildTimestamp>0</pluginBuildTimestamp>

            <!-- put in the URL for the plugin configuration eg. /plugin/Filestack/admin/index -->
            <pluginIframeSrc></pluginIframeSrc>
            <pluginXmlEditorFile>/website/var/config/extension-filestack.xml</pluginXmlEditorFile>


            <!-- className of the plugin which extends Pimcore_API_Plugin_Abstract-->
			<pluginClassName>\Filestack\Plugin</pluginClassName>

            <!-- include paths relative to plugin-directory -->
			<pluginIncludePaths>
				<path>/Filestack/lib</path>
			</pluginIncludePaths>

			<!-- namespaces to register with autoloader-->
			<pluginNamespaces>
				<namespace>Filestack</namespace>
			</pluginNamespaces>

			<!-- js files with path relative to plugin-directory -->
			<pluginJsPaths>
                <path>/plugins/Filestack/static/js/startup.js</path>
			</pluginJsPaths>

			<!-- css files with path relative to plugin-directory -->
			<pluginCssPaths>
                <path>/plugins/Filestack/static/css/backend.css</path>
			</pluginCssPaths>
		</plugin>
</zend-config>
